<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <context-param>
  	<param-name>CGST</param-name>
  	<param-value>0.18</param-value>
  </context-param>
  <servlet>
  	<servlet-name>one</servlet-name>
  	<servlet-class>product_servlet.controller.SaveProduct</servlet-class>
  	
  		<init-param>
  			<param-name>SGST1</param-name>
  			<param-value>0.13</param-value>
  		</init-param>
  		
  		<init-param>
  			<param-name>SGST2</param-name>
  			<param-value>0.1</param-value>
  		</init-param>
  </servlet>
  <servlet-mapping>
  	<servlet-name>one</servlet-name>
  	<url-pattern>/productInfo</url-pattern>
  </servlet-mapping>
</web-app>
